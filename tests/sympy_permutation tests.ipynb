    "from sympy.combinatorics import Permutation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "p2 = Permutation([[1,2],[3,4,5]], size=7)\n",
    "p1 = Permutation([[2,3,4]], size=7)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [
    {
     "output_type": "stream",
     "name": "stdout",
     "text": [
      "(p1*p2)^i ) = (6)\n(p1*p2)^i ) = (6)(1 2 4)(3 5)\n(p1*p2)^i ) = (6)(1 4 2)\n(p1*p2)^i ) = (6)(3 5)\n(p1*p2)^i ) = (6)(1 2 4)\n(p1*p2)^i ) = (6)(1 4 2)(3 5)\n"
     ]
    }
   ],
   "source": [
    "for i in range((p1*p2).order()):\n",
    "    print(\"(p1*p2)^i ) =\",(p1*p2)**i)"
   ]
  }
 ]
